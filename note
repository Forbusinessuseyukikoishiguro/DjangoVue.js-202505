from .models import User

class UserManagerTest(TestCase):
    """UserManagerã®ãƒ†ã‚¹ãƒˆã‚¯ãƒ©ã‚¹"""
    
    def test_create_adult_user_success(self):
        """æˆäººãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆã®æ­£å¸¸ç³»ãƒ†ã‚¹ãƒˆ"""
        # 18æ­³ä»¥ä¸Šã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆ
        user = User.objects.create_adult_user(
            first_name='å¤ªéƒ',           # åå‰ã‚’æŒ‡å®š
            last_name='ç”°ä¸­',            # å§“ã‚’æŒ‡å®š
            birth_date=date(1990, 5, 15)  # 1990å¹´ç”Ÿã¾ã‚Œï¼ˆ30æ­³ä»¥ä¸Šï¼‰
        )
        
        # ãƒ¦ãƒ¼ã‚¶ãƒ¼ãŒæ­£å¸¸ã«ä½œæˆã•ã‚ŒãŸã‹ãƒã‚§ãƒƒã‚¯
        self.assertIsNotNone(user)  # userãŒNoneã§ãªã„ã“ã¨ã‚’ç¢ºèª
        self.assertEqual(user.first_name, 'å¤ªéƒ')  # åå‰ãŒæ­£ã—ãè¨­å®šã•ã‚Œã¦ã„ã‚‹ã‹
        self.assertEqual(user.last_name, 'ç”°ä¸­')   # å§“ãŒæ­£ã—ãè¨­å®šã•ã‚Œã¦ã„ã‚‹ã‹
        self.assertEqual(user.birth_date, date(1990, 5, 15))  # ç”Ÿå¹´æœˆæ—¥ãŒæ­£ã—ãè¨­å®šã•ã‚Œã¦ã„ã‚‹ã‹
        
        # ãƒ‡ãƒ¼ã‚¿ãƒ™ãƒ¼ã‚¹ã«ä¿å­˜ã•ã‚Œã¦ã„ã‚‹ã‹ãƒã‚§ãƒƒã‚¯
        self.assertTrue(User.objects.filter(pk=user.pk).exists())  # DBã«å­˜åœ¨ã™ã‚‹ã‹ç¢ºèª
    
    def test_create_adult_user_boundary_18_years_old(self):
        """18æ­³ã¡ã‚‡ã†ã©ã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆãƒ†ã‚¹ãƒˆ"""
        # ç¾åœ¨å¹´ã‹ã‚‰18å¹´å‰ã®æ—¥ä»˜ã‚’è¨ˆç®—
        current_year = date.today().year  # ç¾åœ¨ã®å¹´ã‚’å–å¾—
