    def test_create_adult_user_multiple_users(self):
        """è¤‡æ•°ã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆãƒ†ã‚¹ãƒˆ"""
        # è¤‡æ•°ã®æˆäººãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä½œæˆ
        users_data = [
            {
                'first_name': 'ä¸€éƒ',
                'last_name': 'æ±äº¬',
                'birth_date': date(1985, 3, 20)
            },
            {
                'first_name': 'äºŒéƒ',
                'last_name': 'å¤§é˜ª',
                'birth_date': date(1992, 7, 10)
            },
            {
                'first_name': 'ä¸‰éƒ',
                'last_name': 'åå¤å±‹',
                'birth_date': date(1988, 11, 5)
            }
        ]
        
        created_users = []  # ä½œæˆã•ã‚ŒãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã®ãƒªã‚¹ãƒˆ
        
        # å„ãƒ¦ãƒ¼ã‚¶ãƒ¼ãƒ‡ãƒ¼ã‚¿ã§ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä½œæˆ
        for user_data in users_data:
            user = User.objects.create_adult_user(**user_data)  # ãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆ
            created_users.append(user)  # ãƒªã‚¹ãƒˆã«è¿½åŠ 
        
        # å…¨ã¦ã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ãŒä½œæˆã•ã‚ŒãŸã‹ãƒã‚§ãƒƒã‚¯
        self.assertEqual(len(created_users), 3)  # 3äººã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ãŒä½œæˆã•ã‚ŒãŸã‹
        
        # ãƒ‡ãƒ¼ã‚¿ãƒ™ãƒ¼ã‚¹ã«ä¿å­˜ã•ã‚Œã¦ã„ã‚‹ã‹ãƒã‚§ãƒƒã‚¯
        db_users = User.objects.all()  # å…¨ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’å–å¾—
        self.assertEqual(db_users.count(), 3)  # ãƒ‡ãƒ¼ã‚¿ãƒ™ãƒ¼ã‚¹ã«3äººä¿å­˜ã•ã‚Œã¦ã„ã‚‹ã‹
        
        # å„ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®æƒ…å ±ãŒæ­£ã—ã„ã‹ãƒã‚§ãƒƒã‚¯
        for i, user in enumerate(created_users):
            expected_data = users_data[i]  # æœŸå¾…ã•ã‚Œã‚‹ãƒ‡ãƒ¼ã‚¿
            self.assertEqual(user.first_name, expected_data['first_name'])
            self.assertEqual(user.last_name, expected_data['last_name'])
            self.assertEqual(user.birth_date, expected_data['birth_date'])
