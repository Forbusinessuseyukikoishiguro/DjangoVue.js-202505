from django.db import models
from datetime import date

class UserManager(models.Manager):
    """ã‚«ã‚¹ã‚¿ãƒ ãƒ¦ãƒ¼ã‚¶ãƒ¼ãƒãƒãƒ¼ã‚¸ãƒ£ãƒ¼"""
    
    @classmethod
    def create_adult_user(cls, **kwargs):
        """
        18æ­³ä»¥ä¸Šã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®ã¿ä½œæˆã™ã‚‹ã‚¯ãƒ©ã‚¹ãƒ¡ã‚½ãƒƒãƒ‰
        Args:
            **kwargs: ãƒ¦ãƒ¼ã‚¶ãƒ¼ä½œæˆã«å¿…è¦ãªãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰
        Returns:
            User: ä½œæˆã•ã‚ŒãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆ
        Raises:
            ValueError: ç”Ÿå¹´æœˆæ—¥ãŒæœªè¨­å®šã¾ãŸã¯18æ­³æœªæº€ã®å ´åˆ
        """
        birth_date = kwargs.get('birth_date')  # ç”Ÿå¹´æœˆæ—¥ã‚’å–å¾—
        
        # ç”Ÿå¹´æœˆæ—¥ãŒè¨­å®šã•ã‚Œã¦ã„ã‚‹ã‹ãƒã‚§ãƒƒã‚¯
        if not birth_date:
            raise ValueError('ç”Ÿå¹´æœˆæ—¥ã¯å¿…é ˆã§ã™')  # ä¾‹å¤–ã‚’ç™ºç”Ÿ
        
        # å¹´é½¢ã‚’è¨ˆç®—
        age = date.today().year - birth_date.year  # ç¾åœ¨å¹´ - èª•ç”Ÿå¹´
        
        # 18æ­³æœªæº€ã®å ´åˆã¯ä¾‹å¤–ã‚’ç™ºç”Ÿ
        if age < 18:
            raise ValueError('18æ­³æœªæº€ã¯ç™»éŒ²ã§ãã¾ã›ã‚“')
        
        # æ¡ä»¶ã‚’æº€ãŸã™å ´åˆã¯ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä½œæˆ
        return cls.create(**kwargs)  # è¦ªã‚¯ãƒ©ã‚¹ã®createãƒ¡ã‚½ãƒƒãƒ‰ã‚’å‘¼ã³å‡ºã—

class User(models.Model):
    """ãƒ¦ãƒ¼ã‚¶ãƒ¼ãƒ¢ãƒ‡ãƒ«"""
    first_name = models.CharField(max_length=50)  # åå‰
    last_name = models.CharField(max_length=50)   # å§“
    birth_date = models.DateField()               # ç”Ÿå¹´æœˆæ—¥
    
    objects = UserManager()  # ã‚«ã‚¹ã‚¿ãƒ ãƒãƒãƒ¼ã‚¸ãƒ£ãƒ¼ã‚’ä½¿ç”¨
    
    def __str__(self):
        return f"{self.first_name} {self.last_name}"
